{
	"name": "@neovici/cosmoz-input",
	"version": "1.7.0",
	"description": "A input web component",
	"keywords": [
		"lit-html",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-input#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-input/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-input.git"
	},
	"license": "Apache-2.0",
	"author": "",
	"main": "cosmoz-input.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"lint-tsc": "tsc",
		"start": "npm run storybook",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"prepare": "husky install",
		"storybook": "start-storybook --node-resolve --watch --open",
		"storybook:build": "build-storybook",
		"storybook:deploy": "storybook-to-ghpages"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"*.js",
		"lib/**/*.js"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"@neovici/cosmoz-utils": "^3.23.0",
		"haunted": "^4.8.0",
		"lit-html": "^1.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@neovici/cfg": "^1.13.1",
		"@open-wc/demoing-storybook": "^2.1.0",
		"@open-wc/testing": "^2.5.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@storybook/storybook-deployer": "^2.8.5",
		"husky": "^7.0.0",
		"semantic-release": "^19.0.0",
		"sinon": "^13.0.0",
		"typescript": "^4.6.0"
	}
}
